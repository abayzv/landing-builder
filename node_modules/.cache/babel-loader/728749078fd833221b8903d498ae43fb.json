{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  data: () => ({\n    form: {\n      email: '',\n      password: '',\n      confpassword: ''\n    },\n    message: \"\"\n  }),\n  computed: { ...mapGetters(['isAuth'])\n  },\n  methods: { ...mapActions('auth', ['register']),\n\n    async _register() {\n      try {\n        await this.register(this.form);\n        this.$router.replace({\n          name: 'home'\n        });\n      } catch (e) {\n        this.message = e.response.data.messages;\n      }\n    }\n\n  },\n\n  created() {\n    if (this.isAuth) {\n      this.$router.replace({\n        name: 'signin'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAgFI,SAAQA,UAAR,EAAoBC,UAApB,QAAqC,MAArC;AACA,eAAe;EACXC,IAAG,EAAI,OAAO;IACVC,IAAI,EAAE;MACFC,KAAK,EAAE,EADL;MAEFC,QAAQ,EAAE,EAFR;MAGFC,YAAY,EAAE;IAHZ,CADI;IAMVC,OAAO,EAAE;EANC,CAAP,CADI;EASXC,QAAQ,EAAE,EACN,GAAGR,UAAU,CAAC,CAAC,QAAD,CAAD;EADP,CATC;EAYXS,OAAO,EAAE,EACL,GAAGR,UAAU,CAAC,MAAD,EAAS,CAAC,UAAD,CAAT,CADR;;IAEL,MAAMS,SAAN,GAAkB;MACd,IAAI;QACA,MAAM,KAAKC,QAAL,CAAc,KAAKR,IAAnB,CAAN;QACA,KAAKS,OAAL,CAAaC,OAAb,CAAqB;UAACC,IAAI,EAAE;QAAP,CAArB;MACJ,CAHA,CAGE,OAAOC,CAAP,EAAU;QACR,KAAKR,OAAL,GAAeQ,CAAC,CAACC,QAAF,CAAWd,IAAX,CAAgBe,QAA/B;MACJ;IACJ;;EATK,CAZE;;EAuBXC,OAAO,GAAE;IACL,IAAG,KAAKC,MAAR,EAAgB;MACZ,KAAKP,OAAL,CAAaC,OAAb,CAAqB;QAACC,IAAI,EAAE;MAAP,CAArB;IACJ;EACJ;;AA3BW,CAAf","names":["mapGetters","mapActions","data","form","email","password","confpassword","message","computed","methods","_register","register","$router","replace","name","e","response","messages","created","isAuth"],"sourceRoot":"","sources":["C:\\Users\\Aji\\Desktop\\builder\\src\\views\\auth\\Register.vue"],"sourcesContent":["<template>\n    <div class=\"register\">\n      <div class=\"flex justify-center\">\n          <div\n            class=\"p-6 max-w-sm bg-white rounded-lg border border-gray-200 shadow-md dark:bg-gray-800 dark:border-gray-700\"\n          >\n            <div class=\"mb-5 text-center\">\n              <h5\n              class=\"mb-2 text font-bold tracking-tight text-gray-900 dark:text-white\"\n            >\n              Register\n            </h5>\n            <p>Silahkan isi form dibawah ini untuk melanjutkan pendaftaran</p>\n            </div>\n            <form @submit.prevent=\"_register\">\n                <div>\n              <label\n                for=\"email\"\n                class=\"block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300\"\n                >Email</label\n              >\n              <div class=\"text-sm text-red-500 mb-2\" v-if=\"message\">{{message.email}}</div>\n              <input\n                v-model=\"form.email\"\n                autocomplete=\"email\"\n                type=\"text\"\n                id=\"email\"\n                class=\"bg-gray-50 border mb-3 border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\n                placeholder=\"Masukan email\"\n                required\n              />\n            </div>\n  \n            <div>\n              <label\n                for=\"password\"\n                class=\"block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300\"\n                >Password</label\n              >\n              <div class=\"text-sm text-red-500 mb-2\" v-if=\"message\">{{message.password}}</div>\n              <input\n              v-model=\"form.password\"\n                type=\"password\"\n                autocomplete=\"new-password\"\n                id=\"password\"\n                class=\"bg-gray-50 border mb-3 border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\n                placeholder=\"Masukan password\"\n                required\n              />\n            </div>\n            <div>\n              <label\n                for=\"conf_password\"\n                class=\"block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300\"\n                >Konfirmasi Password</label\n              >\n              <div class=\"text-sm text-red-500 mb-2\" v-if=\"message\">{{message.confpassword}}</div>\n              <input\n              v-model=\"form.confpassword\"\n                type=\"password\"\n                autocomplete=\"new-password\"\n                id=\"conf_password\"\n                class=\"bg-gray-50 border mb-3 border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\n                placeholder=\"Konfirmasi password\"\n                required\n              />\n            </div>\n            <button type=\"submit\"\n              href=\"#\"\n              class=\"mt-5 inline-flex items-center py-2 px-3 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\n            >\n              Register\n            </button>\n            </form>\n          </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import {mapGetters ,mapActions} from 'vuex'\n    export default {\n        data : () => ({\n            form: {\n                email: '',\n                password: '',\n                confpassword: ''\n            },\n            message: \"\" ,\n        }),\n        computed: {\n            ...mapGetters(['isAuth'])\n        },\n        methods: {\n            ...mapActions('auth', ['register']),\n            async _register() {\n                try {\n                    await this.register(this.form)\n                    this.$router.replace({name: 'home'})\n                } catch (e) {\n                    this.message = e.response.data.messages\n                }\n            }\n        },\n        created(){\n            if(this.isAuth) {\n                this.$router.replace({name: 'signin'})\n            }\n        }\n    }\n</script>"]},"metadata":{},"sourceType":"module"}